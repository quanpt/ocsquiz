    "@types/react-dom": "17.0.3",
    "@types/react-router-dom": "5.1.7",
    "nodemon": "2.0.7",
    "typescript": "4.2.4"
  },
  "scripts": {
    "build": "node ./buildScript",
    "build-front": "react-scripts build",
    "eject": "react-scripts eject",
