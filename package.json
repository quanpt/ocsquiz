{
  "name": "ocquiz_react",
  "version": "0.1.0",
  "description": "App for OCQuiz built with React, Express and SQLite.",
  "private": true,
  "main": "src/index.tsx",
  "proxy": "http://localhost:4001",
  "dependencies": {
    "@material-ui/core": "*",
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@testing-library/user-event": "*",
    "axios": "*",
    "body-parser": "*",
    "compression": "*",
    "cors": "*",
    "dompurify": "*",
    "express": "*",
    "formik": "*",
    "helmet": "*",
    "knex": "*",
    "react": "*",
    "react-dom": "*",
    "react-router": "*",
    "react-router-dom": "*",
    "react-scripts": "*",
    "web-vitals": "*"
  },
  "devDependencies": {
    "@types/dompurify": "*",
    "@types/express": "*",
    "@types/jsdom": "*",
    "@types/react-dom": "*",
    "@types/react-router-dom": "*",
    "concurrently": "*",
    "nodemon": "*",
    "react-scripts": "*",
    "sqlite3": "*",
    "typescript": "*"
  },
  "scripts": {
    "build": "node ./buildScript",
    "build-front": "react-scripts build",
    "eject": "react-scripts eject",
    "start-server": "nodemon server/server.js --watch server/*",
    "start-front": "react-scripts start",
    "start": "concurrently \"npm run start-server\" \"npm run start-front\" --kill-others --kill-others-on-fail",
    "test": "react-scripts test --env=jsdom"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
