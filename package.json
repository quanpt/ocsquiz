    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "13.1.5",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "concurrently": "6.0.2",
    "cors": "2.8.5",
    "cssjson": "^2.1.3",
    "dompurify": "2.2.7",
    "express": "4.17.1",
    "formik": "2.2.6",
    "helmet": "4.5.0",
    "js-cookie": "^2.2.1",
    "knex": "0.95.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "sqlite3": "5.0.2",
    "web-vitals": "1.1.1"
  },
  "devDependencies": {
    "@types/dompurify": "2.2.1",
    "@types/express": "4.17.11",
    "@types/js-cookie": "^2.2.6",
